<?xml version="1.0"?>
<configuration>
	<appSettings/>
	<connectionStrings>
  <add name="MoneyPacificConnectionString" connectionString="Data Source=localhost;Initial Catalog=MoneyPacific;Persist Security Info=True;User ID=sa;Password=admin"
   providerName="System.Data.SqlClient" />
  <add name="MoneyPacificConnectionString1" connectionString="Data Source=.\SQLEXPRESS;AttachDbFilename=|DataDirectory|\MoneyPacific.mdf;Integrated Security=True;User Instance=True"
   providerName="System.Data.SqlClient" />
  <add name="MoneyPacificConnectionString2" connectionString="Data Source=ALEX-PC;Initial Catalog=MoneyPacific;Integrated Security=True"
   providerName="System.Data.SqlClient" />
 </connectionStrings>
	<system.web>
		<compilation debug="true" targetFramework="4.0">
		</compilation>
		<!--
        The <authentication> section enables configuration 
        of the security authentication mode used by 
        ASP.NET to identify an incoming user. 
    -->
		<authentication mode="Windows"/>
		<!--
        The <customErrors> section enables configuration 
        of what to do if/when an unhandled error occurs 
        during the execution of a request. Specifically, 
        it enables developers to configure html error pages 
        to be displayed in place of a error stack trace.

        <customErrors mode="RemoteOnly" defaultRedirect="GenericErrorPage.htm">
         <error statusCode="403" redirect="NoAccess.htm" />
         <error statusCode="404" redirect="FileNotFound.htm" />
        </customErrors>
    -->
		<pages controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID"/></system.web>
	<!-- 
      The system.webServer section is required for running ASP.NET AJAX under Internet
      Information Services 7.0.  It is not necessary for previous version of IIS.
  -->
	<system.serviceModel>
		<services>
			<service behaviorConfiguration="MoneyPacificSrv.Service1Behavior" name="MoneyPacificSrv.Service1">
				<endpoint address="" binding="wsHttpBinding" contract="MoneyPacificSrv.IService1">
					<identity>
						<dns value="localhost"/>
					</identity>
				</endpoint>
				<endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
			</service>
			<service behaviorConfiguration="MoneyPacificSrv.MainBehavior" name="MoneyPacificSrv.Main">
				<endpoint address="" binding="wsHttpBinding" contract="MoneyPacificSrv.IMain">
					<identity>
						<dns value="localhost"/>
					</identity>
				</endpoint>
				<endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
			</service>
			<service behaviorConfiguration="MoneyPacificSrv.MoneyPacificBehavior" name="MoneyPacificSrv.MoneyPacific">
				<endpoint address="" binding="wsHttpBinding" contract="MoneyPacificSrv.IMoneyPacific">
					<identity>
						<dns value="localhost"/>
					</identity>
				</endpoint>
				<endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
			</service>
		</services>
		<behaviors>
			<serviceBehaviors>
				<behavior name="MoneyPacificSrv.Service1Behavior">
					<serviceMetadata httpGetEnabled="true"/>
					<serviceDebug includeExceptionDetailInFaults="false"/>
				</behavior>
				<behavior name="MoneyPacificSrv.MainBehavior">
					<serviceMetadata httpGetEnabled="true"/>
					<serviceDebug includeExceptionDetailInFaults="true"/>
				</behavior>
				<behavior name="MoneyPacificSrv.MoneyPacificBehavior">
					<serviceMetadata httpGetEnabled="true"/>
					<serviceDebug includeExceptionDetailInFaults="false"/>
				</behavior>
			</serviceBehaviors>
		</behaviors>
	</system.serviceModel>
</configuration>
